 <Col>${cart.totalPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                {error && <Message variant=\"danger\">{error}</Message>}\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Button\n                  type=\"button\"\n                  className=\"btn-block\"\n                  disabled={cart.cartItems === 0}\n                  onClick={placeOrderHandler}\n                >\n                  Place Order\n                </Button>\n              </ListGroup.Item>\n            </ListGroup>\n          </Card>\n        </Col>\n      </Row>\n    </>\n  );\n};\n\nexport default PlaceOrderPage;\n","/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/organicstore/organicstore-client/src/store/reducers/orderReducers.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/organicstore/organicstore-client/src/store/constants/orderConstants.js",[],"/Users/abrahamotoo/WEB dev/JS PROJECTS/web sites projects/organicstore/organicstore-client/src/store/actions/orderActions.js",[],{"ruleId":"146","replacedBy":"147"},{"ruleId":"148","replacedBy":"149"},{"ruleId":"150","severity":1,"message":"151","line":2,"column":35,"nodeType":"152","messageId":"153","endLine":2,"endColumn":41},{"ruleId":"150","severity":1,"message":"154","line":2,"column":50,"nodeType":"152","messageId":"153","endLine":2,"endColumn":54},{"ruleId":"150","severity":1,"message":"155","line":3,"column":18,"nodeType":"152","messageId":"153","endLine":3,"endColumn":23},{"ruleId":"150","severity":1,"message":"154","line":2,"column":10,"nodeType":"152","messageId":"153","endLine":2,"endColumn":14},{"ruleId":"156","severity":1,"message":"157","line":40,"column":6,"nodeType":"158","endLine":40,"endColumn":24,"suggestions":"159"},"no-native-reassign",["160"],"no-negated-in-lhs",["161"],"no-unused-vars","'Switch' is defined but never used.","Identifier","unusedVar","'Link' is defined but never used.","'Brand' i